<?xml version="1.0"?>
<component name="org.nuxeo.ipv.callback.rest">

  <extension point="openUrl"
             target="org.nuxeo.ecm.platform.ui.web.auth.service.PluggableAuthenticationService">
    <openUrl name="ipvCallback">
      <method>POST</method>
      <grantPattern>${org.nuxeo.ecm.contextPath}/site/ipv/callback</grantPattern>
    </openUrl>
    <openUrl name="ipvCallbackFile">
      <method>POST</method>
      <grantPattern>${org.nuxeo.ecm.contextPath}/site/ipv/callbackfile</grantPattern>
    </openUrl>
  </extension>

</component>
